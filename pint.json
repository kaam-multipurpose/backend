{
    "rules": {
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "case",
                "constant",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property",
                "property_public_static",
                "property_protected_static",
                "property_private_static",
                "property_static",
                "property_public",
                "property_protected",
                "property_private",
                "construct",
                "method",
                "method_public_static",
                "method_protected_static",
                "method_private_static",
                "method_static",
                "method_public",
                "method_protected",
                "method_private"
            ]
        }
    }
}
